{
    "name": "@gtindependence/nestjs-common",
    "version": "21.1.0",
    "description": "Our common decorators, services, etc for NestJS projects",
    "keywords": [
        "Typescript",
        "NestJS",
        "Seed"
    ],
    "main": "index.js",
    "typings": "index.d.ts",
    "author": "TeamMaestro + GTIndependence",
    "license": "ISC",
    "homepage": "https://github.com/gtindependence/nestjs-common",
    "repository": {
        "type": "git",
        "url": "https://github.com/gtindependence/nestjs-common.git"
    },
    "bugs": {
        "url": "https://github.com/gtindependence/nestjs-common/issues"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "tsc -w",
        "start": "npm run watch",
        "clean": "rm -f *.js && rm -f *.d.ts && rm -Rf modules",
        "build": "npm run clean && tsc -d",
        "prepack": "npm run build",
        "postpack": "npm run clean",
        "postpublish": "git push && git push --tags",
        "pack:dev": "npm pack",
        "postpack:dev": "npm run build && mkdir -p ./build && mv ./*.tgz ./build && echo \"\nPack file path:\n$(pwd)/build/$(ls -tp ./build | grep -v '/$' | head -n 1)\n\""
    },
    "peerDependencies": {
        "@nestjs/common": "^9.4.3",
        "@nestjs/core": "^9.4.3",
